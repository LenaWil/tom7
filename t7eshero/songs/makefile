ALL = ../genscore.exe
#  makefile.songs

# XXX -noxmeasure used to be the default, so I did not install it in these
# in case it makes a formerly good song worse. It should usually result in
# better results, so try it!

# add more...

googscore.mid : goog.mid $(ALL)
	../genscore.exe -watermark 5 -noxmeasure -history 4 -hammertime 26 $< 0 2 4 5 7 8 -o $@
# XXXXXX
#	../genscore.exe -watermark 2 -noxmeasure -history 4 -hammertime 26 $< 0 -o $@

