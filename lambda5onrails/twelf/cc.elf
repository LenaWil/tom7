% freezing needs to preserve closedness
permaclosed : 
      {F : {v:ccval} freeze ([x] N x) ([x] N' v x) (Z v)}
      {E : {x} {y} ccexp-eq (N'' y) (N' x y)}
      type.
%mode permaclosed +F -E.

permavclosed : 
      {F : {v:ccval} vfreeze ([x] V x) ([x] V' v x) (Z v)}
      {E : {x} {y} ccval-eq (V'' y) (V' x y)}
      type.
%mode permavclosed +F -E.

