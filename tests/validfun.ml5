unit
    import "std.mlh"

    extern val alert : string -> unit @ home

    extern bytecode world server
    extern val server ~ server addr

    fun vswap (a : string, b : string) = (b, a)
        
    val (x, y) = from server get vswap ("hello", "world") 

    val (x, y) = vswap (x, y)

    do  alert [[x] [y]]
in
end
